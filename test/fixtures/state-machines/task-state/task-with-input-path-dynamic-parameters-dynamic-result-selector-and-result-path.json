{
  "Comment": "Example of the Pass State",
  "StartAt": "TaskState",
  "States": {
    "TaskState": {
      "Type": "Task",
      "Resource": "module:passThrough",
      "InputPath": "$.theOtherGeorefOf",
      "Parameters": {
        "x-datum": 0,
        "y-datum": 600,
        "where.$": "$.georefOf"
      },
      "ResultSelector": {
        "x-datum.$": "$['x-datum']",
        "y-datum.$": "$['y-datum']",
        "place.$": "$.where"
      },
      "ResultPath": "$.place",
      "End": true
    }
  }
}
